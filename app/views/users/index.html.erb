<div class="container">
    <h1>ユーザ一覧</h1>
    <p>ユーザを一覧で表示します</p>
    <table border="1">
        <tr>
            <th>ID</th>
            <th>名前</th>
            <th>登録日</th>
        </tr>
        <% @users.each do |user| %>
            <tr>
                <td><%=user.id %></td>
                <td><%=link_to("#{user.name}","/users/#{user.id}")%></td>
                <td><%=user.created_at%></td>
            </tr>
        <% end %>
    </table>
</div>
